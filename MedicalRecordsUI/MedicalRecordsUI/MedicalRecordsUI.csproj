<Project Sdk="Uno.Sdk">
    <PropertyGroup>
        <TargetFrameworks>net10.0-ios;net10.0-desktop</TargetFrameworks>

        <OutputType>Exe</OutputType>
        <UnoSingleProject>true</UnoSingleProject>

        <!-- Display name -->
        <ApplicationTitle>MedicalRecordsUI</ApplicationTitle>
        <!-- App Identifier -->
        <ApplicationId>com.companyname.MedicalRecordsUI</ApplicationId>
        <!-- Versions -->
        <ApplicationDisplayVersion>1.0</ApplicationDisplayVersion>
        <ApplicationVersion>1</ApplicationVersion>
        <!-- Package Publisher -->
        <ApplicationPublisher>O=MedicalRecordsUI</ApplicationPublisher>
        <!-- Package Description -->
        <Description>MedicalRecordsUI powered by Uno Platform.</Description>

        <!--
          UnoFeatures are used to quickly add and manage features enabled for your app.
          https://aka.platform.uno/singleproject-features
        -->
        <UnoFeatures>
            Hosting;
            Logging;
            Mvvm;
            Configuration;
            HttpRefit;
            Serialization;
            SkiaRenderer;
        </UnoFeatures>
    </PropertyGroup>

</Project>
